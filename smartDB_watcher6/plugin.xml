      <actionSet
            label="Media Deployer"
            visible="true"
            id="smartdb_watcher.actionset">
         <menu
               label="Media Deployer"
               id="org.eclipse.ui.articles.action.contribution.navigator.subMenu">
            <separator
                  name="Media_Deployer_Group">
            </separator>
         </menu>
